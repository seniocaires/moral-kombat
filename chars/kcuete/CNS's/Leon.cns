[Data]
life = 1000 		;You can easily get accurate value for NeoGeo KOF's using Artmoney, and XI's are around the internet.
attack = 100		;Leave it alone for compatibility.
defence = 100		;Leave it alone for compatibility.
fall.defence_up = 0	;0 for accuracy.
liedown.time = 9		;It's aways 9 in KOF games. SVC uses 12, probably for Demitri's Pursuit Attack to work, but I suggest to leave it alone.
airjuggle = 15		;We'll not use it, but aways keep it for compatibility.
sparkno = 2		;Leave it alone for compatibility.
guard.sparkno = 40	;Leave it alone for compatibility.
KO.echo = 0		;KOF does not use it, NGBC does.
volume = -60		;Manage your global sound volume here.

;Variables with this index and above will not have their values
;reset to 0 between rounds or matches. There are 60 int variables,
;indexed from 0 to 59, and 40 float variables, indexed from 0 to 39.
;If omitted, then it defaults to 60 and 40 for integer and float
;variables repectively, meaning that none are persistent, i.e. all
;are reset. If you want your variables to persist between matches,
;you need to override state 5900 from common1.cns.
IntPersistIndex = 58	;
FloatPersistIndex = 30

